{
  "name": "PayPal Helper",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Automatically unchecks 'Pay for Goods and Services'",
  "homepage_url": "https://github.com/spacetag/PaypalHelper",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.paypal.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}